    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "prettier": "^1.19.1",
    "print-sizes": "^0.1.0",
    "rollup-plugin-vue": "^5.1.9",
    "storybook-addon-preview": "^2.1.1",
    "storybook-dark-mode": "^1.0.7",
    "tslib": "^2.1.0",
    "typescript": "^4.5.0 <4.6.0",
