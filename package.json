{
	"name": "@egjs/infinitegrid",
	"version": "3.4.2",
	"description": "A module used to arrange card elements including content infinitely according to layout type. With this module, you can implement various layouts composed of different card elements whose sizes vary. It guarantees performance by maintaining the number of DOMs the module is handling under any circumstance",
	"sideEffects": false,
	"module": "dist/esm/index",
	"main": "dist/infinitegrid.js",
	"types": "declaration/index.d.ts",
	"scripts": {
		"start": "concurrently 'BABEL_ENV=umd webpack-dev-server --host=0.0.0.0 --env.name=InfiniteGrid --env.path=./src/index.umd.js' 'webpack --watch --env.name=Parallax --env.path=./src/Parallax.js'",
		"build": "rm -rf ./dist && concurrently 'npm run build:infinitegrid' 'npm run build:module' 'npm run build:parallax'",
		"build:module": "BABEL_ENV=esm babel ./src --out-dir ./dist/esm && npm run build:version",
		"build:version": "replace-in-file '#__VERSION__#' $npm_package_version dist/esm/version.js",
		"build:infinitegrid": "BABEL_ENV=umd webpack --env.type=production --env.name=InfiniteGrid --env.path=./src/index.umd.js &&BABEL_ENV=umd webpack --env.type=packaged --env.name=InfiniteGrid --env.path=./src/index.umd.js",
		"build:parallax": "BABEL_ENV=umd webpack --env.type=production --env.name=Parallax --env.path=./src/Parallax.js",
		"test": "BABEL_ENV=umd karma start",
		"test:chrome": "BABEL_ENV=umd karma start --chrome",
		"coverage": "BABEL_ENV=umd karma start --coverage",
		"lint": "eslint src",
		"jsdoc": "rm -rf ./doc && jsdoc -c jsdoc.json",
		"demo:start": "npm run build && npm run demo:prebuild-latest && npm run demo:updateVersion && jekyll serve -s demo --host 0.0.0.0",
		"demo:build": "npm run demo:updateVersion && jekyll build -s demo",
		"demo:prebuild-version": "cpx 'dist/**/*' demo/release/$npm_package_version/dist --clean && cpx 'doc/**/*' demo/release/$npm_package_version/doc --clean",
		"demo:prebuild-latest": "cpx 'dist/**/*' demo/release/latest/dist --clean && cpx 'doc/**/*' demo/release/latest/doc --clean",
		"demo:origin": "npm run build && npm run jsdoc && npm run demo:prebuild-version && npm run demo:prebuild-latest && npm run demo:build && gh-pages -d demo/_site",
		"demo:deploy": "npm run build && npm run jsdoc && npm run demo:prebuild-version && npm run demo:prebuild-latest && npm run demo:build && gh-pages -d demo/_site --add --remote upstream",
		"demo:updateVersion": "echo $npm_package_version > demo/_data/version.yml",
		"demo:setup": "cpx 'node_modules/@egjs/common-demo/**/*' 'demo/' && rm demo/package.json",
		"release": "node config/release.js",
		"changelog": "node ./config/changelog.js",
		"coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
		"prepush": "npm run lint",
		"commitmsg": "node config/validate-commit-msg.js"
	},
	"author": {
		"name": "NAVER Corp."
	},
	"namespace": {
		"eg": "eg"
	},
	"license": "MIT",
	"homepage": "https://github.com/naver/egjs-infinitegrid",
	"dependencies": {
		"@egjs/component": "^2.1.1"
	},
	"keywords": [
		"infinitegrid",
		"infinite",
		"scroll",
		"layout",
		"DOM",
		"html",
		"gallery",
		"grid",
		"pinterest",
		"masonry",
		"GridLayout",
		"JustifiedLayout",
		"PackingLayout",
		"FrameLayout",
		"SquareLayout"
	],
	"devDependencies": {
		"@babel/cli": "^7.0.0",
		"@babel/core": "^7.0.0",
		"@babel/plugin-transform-object-assign": "^7.0.0",
		"@babel/preset-env": "^7.0.0",
		"@egjs/common-demo": "github:naver/egjs#common-demo",
		"babel-loader": "^8.0.0",
		"babel-plugin-add-module-exports": "^0.2.1",
		"babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
		"babel-plugin-transform-es3-property-literals": "^6.22.0",
		"babel-polyfill": "^6.26.0",
		"chai": "^4.1.2",
		"chalk": "^2.3.0",
		"concurrently": "^3.5.1",
		"coveralls": "^3.0.0",
		"cpx": "^1.5.0",
		"egjs-jsdoc-template": "^1.4.2",
		"eslint": "^4.12.1",
		"eslint-config-naver": "^2.0.1",
		"eslint-loader": "^1.9.0",
		"eslint-plugin-import": "^2.8.0",
		"fs-extra": "^4.0.3",
		"gh-pages": "^1.1.0",
		"husky": "^0.14.3",
		"inject-loader": "^4.0.1",
		"istanbul-instrumenter-loader": "^3.0.0",
		"jquery": "^3.2.1",
		"jsdoc": "^3.5.5",
		"karma": "^1.7.1",
		"karma-chai": "^0.1.0",
		"karma-chrome-launcher": "^2.2.0",
		"karma-coverage-istanbul-reporter": "^1.3.0",
		"karma-mocha": "^1.3.0",
		"karma-mocha-reporter": "^2.2.5",
		"karma-sinon": "^1.0.5",
		"karma-sourcemap-loader": "^0.3.7",
		"karma-webpack": "^3.0.0",
		"lite-fixture": "^1.0.2",
		"mocha": "^4.0.1",
		"replace-in-file": "^3.4.2",
		"sinon": "^4.1.2",
		"string-replace-webpack-plugin": "0.1.3",
		"sync-exec": "^0.6.2",
		"uglifyjs-webpack-plugin": "^1.1.2",
		"webpack": "^3.12.0",
		"webpack-dev-server": "^2.9.5",
		"webpack-merge": "^4.1.1",
		"write-file-webpack-plugin": "^4.2.0",
		"xml2js": "^0.4.19"
	}
}
