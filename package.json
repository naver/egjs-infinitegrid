{
	"name": "@egjs/infinitegrid",
	"version": "3.8.2-snapshot",
	"description": "A module used to arrange card elements including content infinitely according to layout type. With this module, you can implement various layouts composed of different card elements whose sizes vary. It guarantees performance by maintaining the number of DOMs the module is handling under any circumstance",
	"sideEffects": false,
	"module": "dist/infinitegrid.esm.js",
	"main": "dist/infinitegrid.js",
	"types": "declaration/index.d.ts",
	"scripts": {
		"start": "rollup -c rollup.config.test.js -w",
		"build": "rm -rf ./dist ./declaration && rollup -c && npm run declaration && npm run declaration:copy && npm run printsizes",
		"declaration": "rm -rf declaration && tsc -p tsconfig.declaration.json",
		"declaration:copy": "cp declaration/Parallax.d.ts dist/parallax.d.ts && cp declaration/types.d.ts dist/types.d.ts",
		"printsizes": "print-sizes ./dist --exclude=\\.map && node ./config/readme.js",
		"build:webpack": "rm -rf ./dist && concurrently 'npm run build:infinitegrid' 'npm run build:parallax'",
		"build:parallax": "webpack --env.type=production --env.name=Parallax --env.path=./src/Parallax.js",
		"test": "karma start",
		"test:chrome": "karma start --chrome",
		"coverage": "karma start --coverage && print-coveralls --sort=desc",
		"lint": "eslint src",
		"jsdoc": "rm -rf ./doc && jsdoc -c jsdoc.json",
		"storybook": "cd storybook && npm run build && cd ../",
		"demo:start": "npm run build && npm run demo:prebuild-latest && npm run demo:updateVersion && jekyll serve -s demo --host 0.0.0.0",
		"demo:build": "npm run demo:updateVersion && jekyll build -s demo",
		"demo:prebuild-version": "cpx 'dist/**/*' demo/release/$npm_package_version/dist --clean && cpx 'doc/**/*' demo/release/$npm_package_version/doc --clean",
		"demo:prebuild-latest": "cpx 'dist/**/*' demo/release/latest/dist --clean && cpx 'doc/**/*' demo/release/latest/doc --clean",
		"demo:predeploy": "npm run build && npm run jsdoc && npm run demo:prebuild-version && npm run demo:prebuild-latest && npm run demo:build && npm run storybook",
		"demo:origin": "npm run demo:predeploy && gh-pages -d demo/_site",
		"demo:deploy": "gh-pages -d demo/_site --add --remote upstream",
		"demo:updateVersion": "echo $npm_package_version > demo/_data/version.yml",
		"demo:setup": "cpx 'node_modules/@egjs/common-demo/**/*' 'demo/' && rm demo/package.json",
		"release": "node config/release.js",
		"changelog": "node ./config/changelog.js",
		"coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
		"prepush": "npm run lint",
		"commitmsg": "node config/validate-commit-msg.js",
		"packages": "npm run packages:update && npm run packages:build && npm run packages:publish",
		"packages:update": "pvu --update=react-infinitegrid,ngx-infinitegrid/projects/ngx-infinitegrid,vue-infinitegrid,svelte-infinitegrid",
		"packages:build": "pvu --build=react-infinitegrid,ngx-infinitegrid/,vue-infinitegrid,svelte-infinitegrid",
		"packages:publish": "pvu --publish=react-infinitegrid,ngx-infinitegrid/dist/ngx-infinitegrid,vue-infinitegrid,svelte-infinitegrid"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/naver/egjs-infinitegrid"
	},
	"author": {
		"name": "NAVER Corp."
	},
	"namespace": {
		"eg": "eg"
	},
	"license": "MIT",
	"homepage": "https://github.com/naver/egjs-infinitegrid",
	"dependencies": {
		"@egjs/component": "^2.2.2",
		"@egjs/imready": "^1.1.2",
		"@egjs/list-differ": "^1.0.0"
	},
	"keywords": [
		"infinitegrid",
		"infinite",
		"scroll",
		"layout",
		"DOM",
		"html",
		"gallery",
		"grid",
		"pinterest",
		"masonry",
		"GridLayout",
		"JustifiedLayout",
		"PackingLayout",
		"FrameLayout",
		"SquareLayout"
	],
	"devDependencies": {
		"@babel/cli": "^7.0.0",
		"@babel/core": "^7.0.0",
		"@babel/plugin-proposal-class-properties": "^7.1.0",
		"@babel/plugin-transform-object-assign": "^7.0.0",
		"@babel/preset-env": "^7.0.0",
		"@daybrush/jsdoc": "^0.3.10",
		"@egjs/build-helper": "0.0.5",
		"@egjs/common-demo": "github:naver/egjs#common-demo",
		"awesome-typescript-loader": "^3.5.0",
		"babel-eslint": "^10.0.1",
		"babel-loader": "^8.0.0",
		"babel-plugin-add-module-exports": "^0.2.1",
		"babel-plugin-no-side-effect-class-properties": "0.0.4",
		"babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
		"babel-plugin-transform-es3-property-literals": "^6.22.0",
		"babel-polyfill": "^6.26.0",
		"chai": "^4.1.2",
		"chalk": "^2.3.0",
		"concurrently": "^3.5.1",
		"coveralls": "^3.0.0",
		"cpx": "^1.5.0",
		"egjs-jsdoc-template": "^1.4.2",
		"eslint": "^4.12.1",
		"eslint-config-naver": "^2.0.1",
		"eslint-loader": "^1.9.0",
		"eslint-plugin-import": "^2.8.0",
		"fs-extra": "^4.0.3",
		"gh-pages": "^1.1.0",
		"husky": "^0.14.3",
		"inject-loader": "^4.0.1",
		"istanbul-instrumenter-loader": "^3.0.0",
		"jquery": "^3.2.1",
		"karma": "^1.7.1",
		"karma-chai": "^0.1.0",
		"karma-chrome-launcher": "^2.2.0",
		"karma-coverage-istanbul-reporter": "^1.3.0",
		"karma-mocha": "^1.3.0",
		"karma-mocha-reporter": "^2.2.5",
		"karma-sinon": "^1.0.5",
		"karma-sourcemap-loader": "^0.3.7",
		"karma-webpack": "^3.0.0",
		"lite-fixture": "^1.0.2",
		"mocha": "^4.0.1",
		"print-coveralls": "^1.2.2",
		"print-sizes": "^0.0.3",
		"pvu": "^0.5.1",
		"replace-in-file": "^3.4.2",
		"rollup": "^0.67.1",
		"rollup-plugin-babel": "^4.0.3",
		"rollup-plugin-node-resolve": "^3.4.0",
		"rollup-plugin-prototype-minify": "^1.0.4",
		"rollup-plugin-replace": "^2.1.0",
		"rollup-plugin-typescript": "^1.0.0",
		"rollup-plugin-uglify": "^6.0.0",
		"sinon": "^4.1.2",
		"string-replace-webpack-plugin": "0.1.3",
		"sync-exec": "^0.6.2",
		"tslint": "^5.12.0",
		"typescript": "^3.9.7",
		"uglifyjs-webpack-plugin": "^1.1.2",
		"webpack": "^3.12.0",
		"webpack-dev-server": "^2.9.5",
		"webpack-merge": "^4.1.1",
		"write-file-webpack-plugin": "^4.2.0",
		"xml2js": "^0.4.19"
	}
}
